{
  "name": "electro_react_study",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "format": "prettier \"./**/*.{js,jsx,ts,tsx,css,scss,less}\" --write",
    "stylelint:fix": "stylelint \"**/*.{css,scss,js,jsx,ts,tsx}\" --cache --fix",
    "lint-all": "pnpm run lint:fix && pnpm run stylelint:fix && pnpm run format",
    "dev:app:web": "pnpm --filter zjp-web dev",
    "dev:app:h5": "pnpm --filter zjp-h5 dev",
    "dev:app:desktop": "pnpm --filter zjp-desktop dev",
    "dev:app:desktop:hot": "pnpm --filter zjp-desktop dev:hot",
    "dev:app:desktop:kill": "pkill -9 Electron && pnpm --filter zjp-desktop dev",
    "dev:app:desktop:mac": "pnpm --filter zjp-desktop dev:mac",
    "dev:app:desktop:win": "pnpm --filter zjp-desktop dev:win",
    "dev:app:embed": "pnpm --filter zjp-embed dev",
    "build:app:embed": "pnpm --filter zjp-embed build",
    "build:app:web": "pnpm --filter zjp-web build",
    "build:app:win": "pnpm --filter zjp-desktop build:win",
    "build:app:mac": "pnpm --filter zjp-desktop build:mac",
    "build:app:mac:test": "pnpm --filter zjp-desktop build:mac:test",
    "build:package:wasm": "pnpm --filter @zjp-web/open-im-wasm build",
    "deploy:mac": "pnpm --filter zjp-desktop deploy:mac",
    "preview": "vite preview",
    "add:ui": "pnpm --filter @zjp-web/components add:ui",
    "migrate": "node ./scripts/migration.js",
    "analyze-deps": "node ./scripts/analyze-deps.js",
    "i18n:check": "pnpm --filter @zjp-web/i18n check:keys",
    "extract-licenses": "node ./scripts/extract-licenses.cjs",
    "package-extension": "node ./scripts/package-extension.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd-mobile": "^5.32.0",
    "antd-mobile-icons": "^0.3.0",
    "less": "^4.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "6.28.0",
    "react-router-dom": "6.28.0",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.1",
    "@types/node": "20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.11",
    "postcss-jsx": "^0.36.4",
    "postcss-preset-env": "^10.1.2",
    "prettier": "^3.4.2",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-styled-components": "^0.1.1",
    "vite": "^6.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
